{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "memio-tauri-example",
  "version": "0.1.0",
  "identifier": "com.pdc.memio-tauri-example",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "create": false,
        "title": "memio-tauri-example",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["desktop"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "../public/sample_sales_data.xlsx"
    ]
  }
}
